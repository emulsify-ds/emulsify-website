{
  "name": "gatsby-contentful-starter",
  "description": "Emulsify website based on Starter Contentful Gatsby Blog",
  "version": "1.0.0",
  "author": "Evan Willhite <evan@fourkitchens.com>",
  "bugs": {
    "url": "https://github.com/contentful-userland/gatsby-contentful-starter/issues"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby-cli": "^2.19.1",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-google-tagmanager": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-source-contentful": "^4.6.1",
    "gatsby-transformer-remark": "^2.16.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sharp": "^0.27.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/classnames": "^2.3.1",
    "@types/css-modules": "^1.0.2",
    "@types/react": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chalk": "^4.1.0",
    "contentful-import": "^7.9.37",
    "dotenv": "^8.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-security": "^1.4.0",
    "gatsby": "^2.32.3",
    "gh-pages": "^3.1.0",
    "husky": "^7.0.0",
    "inquirer": "^7.3.3",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "typescript": "^4.3.5",
    "yargs-parser": "^20.2.4"
  },
  "homepage": "https://github.com/evanmwillhite/emulsify-website/#readme",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanmwillhite/emulsify-website.git"
  },
  "scripts": {
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,md}' 'bin/*.js' && npm run lint -- --fix && npm run stylelint -- --fix",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "noIndex": "npm run build && echo 'User-agent: *\nDisallow: /' > public/robots.txt",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "postinstall": "node ./bin/hello.js",
    "setup": "node ./bin/setup.js",
    "start": "npm run develop",
    "stylelint": "npx stylelint './src/**/*.css'",
    "heroku-postbuild": "gatsby build",
    "cleanup-repository": "yarn remove contentful-import chalk inquirer && node ./bin/cleanup.js && rimraf bin contentful",
    "prepare": "husky install"
  }
}
