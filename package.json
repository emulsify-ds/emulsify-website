{
  "name": "gatsby-contentful-starter",
  "description": "Emulsify website based on Starter Contentful Gatsby Blog",
  "version": "1.0.0",
  "author": "Evan Willhite <evan@fourkitchens.com>",
  "bugs": {
    "url": "https://github.com/contentful-userland/gatsby-contentful-starter/issues"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby-cli": "^2.12.60",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-source-contentful": "^2.2.9",
    "gatsby-transformer-remark": "^2.7.3",
    "gatsby-transformer-sharp": "^2.4.6",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "sharp": "^0.25.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chalk": "^4.0.0",
    "contentful-import": "^7.7.8",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "gatsby": "^2.21.30",
    "gh-pages": "^2.2.0",
    "inquirer": "^7.1.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "yargs-parser": "^18.1.3"
  },
  "homepage": "https://github.com/evanmwillhite/emulsify-website/#readme",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanmwillhite/emulsify-website.git"
  },
  "scripts": {
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "noIndex": "npm run build && echo 'User-agent: *\nDisallow: /' > public/robots.txt",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "postinstall": "node ./bin/hello.js",
    "setup": "node ./bin/setup.js",
    "start": "npm run develop",
    "heroku-postbuild": "gatsby build",
    "cleanup-repository": "yarn remove contentful-import chalk inquirer && node ./bin/cleanup.js && rimraf bin contentful"
  }
}
